<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/timeline_survey">

        <FrameLayout
            android:id="@+id/fr"
            android:layout_width="14.5dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="20dp">

            <LinearLayout
                android:id="@+id/frameLayout"
                android:layout_width="1dp"
                android:layout_height="57dp"
                android:layout_gravity="center"
                android:background="#b0b1b2"
                android:orientation="horizontal" />

        </FrameLayout>

        <ImageView
            android:id="@+id/timeline_icon2"
            android:layout_width="14.5dp"
            android:layout_height="14.5dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/projectnull" />

        <TextView
            android:id="@+id/timeline_survey_t2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/timeline_icon2"
            android:text="사후 서베이 등록"
            android:textColor="@color/grayFont"
            android:textSize="15sp"
            android:layout_marginBottom="7.75dp"
            android:textStyle="bold"/>

        <TextView
            android:id="@+id/timeline_survey_date2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/timeline_survey_t2"
            android:layout_below="@+id/timeline_survey_t2"
            android:text="시작하지않음"
            android:textColor="@color/grayFont"
            android:textSize="12.5sp" />
        <ImageView
            android:id="@+id/timeline_survey_arrow2"
            android:layout_width="11.75dp"
            android:layout_height="18dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:src="@drawable/projectstartbtn"
            android:visibility="gone" />
    </RelativeLayout>
</LinearLayout>